<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".MainActivity">



    <com.mobarok.roundprogressbar.RoundProgressBar
        app:progressDirection="TO_LEFT"
        android:id="@+id/roundedProgressBar"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:progress="11"
        app:progressMax="33"
        app:progressBarBackgroundColor="#F4F4F4"
        app:progressBarColor="#4CAF50"
        app:setRoundBorder="true"
        app:indeterminateMode="true"
        />



    <com.mobarok.roundprogressbar.RoundProgressBar
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:progress="11"
        app:progressMax="33"
        app:progressBarBackgroundColor="#F4F4F4"
        app:progressBarColor="#4CAF50"
        app:setRoundBorder="true"
        />


    <com.mobarok.roundprogressbar.RoundProgressBar
        app:progressDirection="TO_LEFT"
        android:layout_width="100dp"
        android:layout_height="100dp"
        app:progress="200"
        app:progressMax="500"
        app:progressBarBackgroundColor="#F4F4F4"
        app:progressBarColor="#4CAF50"
        app:setRoundBorder="true"
        />

</LinearLayout>